          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT\n\tCOALESCE(geofence.name, CONCAT_WS(', ', COALESCE(address.name, nullif(CONCAT_WS(' ', address.road, address.house_number), '')), address.city)) AS location,\n   SUM(charge_energy_added) AS total_charge_energy_added,\n   SUM(duration_min) AS total_duration_min,\n   SUM(duration_min)/count(*) AS avg_duration,\n   count(*) as charge_number,\n   60*SUM(charge_energy_added)/SUM(duration_min) AS avg_energy,\n   SUM(charge_energy_added)/count(*) AS avg_added\n  \nFROM\n\tcharging_processes c\nLEFT JOIN addresses address ON c.address_id = address.id\nLEFT JOIN geofences geofence ON geofence_id = geofence.id\nWHERE\n\t$__timeFilter(end_date)\n\tAND car_id = $car_id\n\tAND duration_min>0\nGROUP BY\n\t1\nORDER BY\n\ttotal_charge_energy_added DESC;",
